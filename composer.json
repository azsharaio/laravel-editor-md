{
    "name": "azshara/laravel-editor-md",
    "description": "editor.md for laravel",
    "keywords": ["laravel", "editor.md", "markdown", "azshara"],
    "license": "MIT",
    "authors": [
        {
            "name": "azshara",
            "email": "azshara.io@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9"
    },
    "autoload": {
        "psr-4": {
            "Douyasi\\Editor\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Douyasi\\Editor\\EditorServiceProvider"
            ],
            "aliases": {                
                "Editor": "Douyasi\\Editor\\EditorServiceProvider"
            }
        }
    }
}
